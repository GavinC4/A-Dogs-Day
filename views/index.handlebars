    <!-- SIGN UP MODAL -->
    <div id="signup" class="modal">
        <div class="modal-content">
            <div id="signup" class="tab-pane">
                <h1 id="logText">Sign Up for Free</h1>
                <!-- add handlebar object, signup title here -->
                <!-- <h1>{{registrationTitle}}</h1>
                {{#if errors}}
